<h1>New Person</h1>

<%= form_for @person do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <%= f.label :age %>
  <%= f.number_field :age %>
  <%= f.label :gender %>
  <%= f.text_field :gender %>
  <%= f.submit %>
<% end %>